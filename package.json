{
  "name": "fastexpress",
  "version": "1.5.0",
  "module": "./src/index.js",
  "main": "./cjs/index.js",
  "unpkg": "dist/fastexpress.min.js",
  "jsdelivr": "dist/fastexpress.min.js",
  "files": [
    "cjs",
    "src",
    "dist"
  ],
  "repository": "git@github.com:davidcostadev/fastexpress.git",
  "author": "David Costa <davidcostadev@gmail.com>",
  "license": "MIT",
  "scripts": {
    "jest": "jest",
    "test": "jest",
    "build": "npm run build:cjs && npm run build:umd && npm run build:umd:min",
    "build:cjs": "babel src --out-dir cjs",
    "build:umd": "cross-env NODE_ENV=development rollup -c -o dist/fastexpress.js",
    "build:umd:min": "cross-env NODE_ENV=production rollup -c -o dist/fastexpress.min.js",
    "clean": "rimraf dist/* cjs/*",
    "prepare": "npm run clean && npm run build",
    "npm-publish": "npm run build && npm publish"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "babel-types": "^6.26.0",
    "cross-env": "^5.2.0",
    "jest": "^23.6.0",
    "regenerator-runtime": "^0.12.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.66.6",
    "rollup-plugin-terser": "^3.0.0"
  },
  "dependencies": {
    "bcrypt": "^3.0.1",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.3.0",
    "moment": "^2.22.2",
    "ramda": "^0.25.0",
    "sequelize": "^4.39.1"
  },
  "jest": {
    "roots": [
      "<rootDir>/tests"
    ]
  }
}
